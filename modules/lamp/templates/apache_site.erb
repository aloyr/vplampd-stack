RewriteEngine On
RewriteCond %{REQUEST_METHOD} ^TRACE
RewriteRule .* - [F]
TraceEnable off
ServerSignature Off
ServerTokens Prod
<Virtualhost *:80>
  ServerName <%= @webhost %>
  <%= @serveralias %>
  DocumentRoot <%= @webroot %>
  <Directory <%= @webroot %>>
  	EnableSendFile Off
    AllowOverride All
  </Directory>
</Virtualhost>
